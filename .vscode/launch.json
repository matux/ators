{
  // Use IntelliSense to learn about possible attributes.
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'ators'",
      "cargo": {
        "args": [
          "build",
          "--bin=ators",
          "--package=ators"
        ],
        "filter": {
          "name": "ators",
          "kind": "bin"
        }
      },
      "args": [
        "-v",
        "-i",
        "-o=./fixtures/iosAppSwift.app.dSYM/Contents/Resources/DWARF/iosAppSwift",
        "-l=0x0100360000",
        "--",
        "0x0100369e4c"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'ators'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=ators",
          "--package=ators"
        ],
        "filter": {
          "name": "ators",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'atorsl'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=atorsl"
        ],
        "filter": {
          "name": "atorsl",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
